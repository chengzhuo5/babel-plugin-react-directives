{
  "name": "babel-plugin-react-directives",
  "version": "1.2.0-alpha.1",
  "description": "A babel plugin that provides some directives for react(any JSX), similar to directive of vue.",
  "main": "src/index.js",
  "files": [
    "src",
    "runtime",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "test:no-cache": "jest --clearCache && npm run test",
    "test:install": "node test/install.js",
    "pg:install": "node playground/install.js",
    "pg:dev": "node playground/build.js --dev",
    "pg:build": "node playground/build.js",
    "postinstall": "npm run test:install && npm run pg:install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "peakchen90 <1558267774@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peakchen90/babel-plugin-react-directives"
  },
  "bugs": {
    "url": "https://github.com/peakchen90/babel-plugin-react-directives/issues"
  },
  "keywords": [
    "babel-plugin",
    "react-directive",
    "jsx-directive",
    "jsx-plus",
    "directive",
    "react",
    "vue",
    "x-if",
    "x-for",
    "x-class"
  ],
  "engines": {
    "node": ">=8.6.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@babel/generator": "^7.6.4",
    "@babel/types": "^7.6.1",
    "ajv": "^6.10.2",
    "ajv-errors": "^1.0.1",
    "chalk": "^2.4.2",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.19",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.2.3",
    "common-tags": "^1.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-directives": "^1.0.0",
    "husky": "^3.0.7",
    "invariant": "^2.2.2",
    "jest": "^25.2.3",
    "lint-staged": "^9.4.1",
    "lodash.mergewith": "^4.6.0",
    "path-exists": "^4.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "strip-indent": "^3.0.0"
  }
}
